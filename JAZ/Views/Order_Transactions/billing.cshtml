        <script>
                                        var cardType = "";
                                        function ccValidate(ccn) {
                                            var regex_visa = new RegExp("^(4)");
                                            var regex_master = new RegExp("^(51|52|53|54|55)");
                                            var regex_amex = new RegExp("^(34|37)");


                                            if (regex_visa.test(ccn)) {
                                                cardType = "Visa";
                                                $("#master").addClass("transparent");
                                                $("#amex").addClass("transparent");
                                                $("#visa").removeClass("transparent");
                                            }
                                            else if (regex_master.test(ccn)) {
                                                cardType = "Master";
                                                $("#master").removeClass("transparent");
                                                $("#amex").addClass("transparent");
                                                $("#visa").addClass("transparent");
                                            }
                                            else if (regex_amex.test(ccn)) {
                                                cardType = "American Express";
                                                $("#master").addClass("transparent");
                                                $("#amex").removeClass("transparent");
                                                $("#visa").addClass("transparent");
                                            }
                                            else {
                                                $("#master").addClass("transparent");
                                                $("#amex").addClass("transparent");
                                                $("#visa").addClass("transparent");
                                            }
                                            return cardType;
                                        }

                                        $(function () {
                                            $("#CreditCardNo").change(function () {
                                                ccValidate($(this).val());
                                            });
                                            $("#CreditCardNo").keyup(function () {
                                                ccValidate($(this).val());
                                            });

                                            /*document.getElementById('CreditCardNo').oninput = function () {
                                                this.value = cc_format(this.value)
                                            }*/
                                            $("#CreditCardNo").keypress(function (e) {
                                                if ((e.which < 48 || e.which > 57) && (e.which !== 8) && (e.which !== 0)) {
                                                    return false;
                                                }
                                                return true;
                                            });
                                        });


                                        function cc_format(value) {
                                            var v = value.replace(/\s+/g, '').replace(/[^0-9]/gi, '')
                                            var matches = v.match(/\d{4,16}/g);
                                            var match = matches && matches[0] || ''
                                            var parts = []
                                            for (i = 0, len = match.length; i < len; i += 4) {
                                                parts.push(match.substring(i, i + 4))
                                            }
                                            if (parts.length) {
                                                return parts.join(' ')
                                            } else {
                                                return value
                                            }
                                        }

                                        /*  ================================================================
Credit card verification functions
Originally included as Starter Application 1.0.0 in LivePayment.
20 Feb 1997 modified by egk:
changed naming convention to initial lowercase
                                          (isMasterCard instead of IsMasterCard, etc.)
changed isCC to isCreditCard
retained functions named with older conventions from
                                          LivePayment as stub functions for backward
                                          compatibility only
added "AMERICANEXPRESS" as equivalent of "AMEX"
                                          for naming consistency
================================================================ */


                                        /*  ================================================================
                                            FUNCTION:  isCreditCard(st)

                                            INPUT:     st - a string representing a credit card number

                                            RETURNS:  true, if the credit card number passes the Luhn Mod-10
                                                    test.
                                                  false, otherwise
                                            ================================================================ */

                                        function isCreditCard(st) {
                                            // Encoding only works on cards with less than 19 digits
                                            if (st.length > 19)
                                                return (false);

                                            sum = 0; mul = 1; l = st.length;
                                            for (i = 0; i < l; i++) {
                                                digit = st.substring(l - i - 1, l - i);
                                                tproduct = parseInt(digit, 10) * mul;
                                                if (tproduct >= 10)
                                                    sum += (tproduct % 10) + 1;
                                                else
                                                    sum += tproduct;
                                                if (mul == 1)
                                                    mul++;
                                                else
                                                    mul--;
                                            }
                                            // Uncomment the following line to help create credit card numbers
                                            // 1. Create a dummy number with a 0 as the last digit
                                            // 2. Examine the sum written out
                                            // 3. Replace the last digit with the difference between the sum and
                                            //    the next multiple of 10.

                                            //  document.writeln("<BR>Sum      = ",sum,"<BR>");
                                            //  alert("Sum      = " + sum);

                                            if ((sum % 10) == 0)
                                                return (true);
                                            else
                                                return (false);

                                        } // END FUNCTION isCreditCard()

                                        function stripInput(cc) {
                                            cc = cc.replace(/ /g, '');
                                            cc = cc.replace(/-/g, '');
                                            return cc;
                                        }

                                        /*  ================================================================
                                            FUNCTION:  isVisa()

                                            INPUT:     cc - a string representing a credit card number

                                            RETURNS:  true, if the credit card number is a valid VISA number.

                                                  false, otherwise

                                            Sample number: 4111 1111 1111 1111 (16 digits)
                                            ================================================================ */

                                        function isVisa(cc) {
                                            cc = stripInput(cc);
                                            console.log(cc.length)
                                            if (((cc.length == 16) || (cc.length == 13)) &&
                                                (cc.substring(0, 1) == 4))
                                                return isCreditCard(cc);
                                            return false;
                                        }  // END FUNCTION isVisa()




                                        /*  ================================================================
                                            FUNCTION:  isMasterCard()

                                            INPUT:     cc - a string representing a credit card number

                                            RETURNS:  true, if the credit card number is a valid MasterCard
                                                    number.

                                                  false, otherwise

                                            Sample number: 5500 0000 0000 0004 (16 digits)
                                            ================================================================ */

                                        function isMasterCard(cc) {
                                            cc = stripInput(cc);
                                            firstdig = cc.substring(0, 1);
                                            seconddig = cc.substring(1, 2);
                                            if ((cc.length == 16) && (firstdig == 5) &&
                                                ((seconddig >= 1) && (seconddig <= 5)))
                                                return isCreditCard(cc);
                                            return false;

                                        } // END FUNCTION isMasterCard()





                                        /*  ================================================================
                                            FUNCTION:  isAmericanExpress()

                                            INPUT:     cc - a string representing a credit card number

                                            RETURNS:  true, if the credit card number is a valid American
                                                    Express number.

                                                  false, otherwise

                                            Sample number: 340000000000009 (15 digits)
                                            ================================================================ */

                                        function isAmericanExpress(cc) {
                                            cc = stripInput(cc);
                                            firstdig = cc.substring(0, 1);
                                            seconddig = cc.substring(1, 2);
                                            if ((cc.length == 15) && (firstdig == 3) &&
                                                ((seconddig == 4) || (seconddig == 7)))
                                                return isCreditCard(cc);
                                            return false;

                                        } // END FUNCTION isAmericanExpress()

                                        function confirmPayment() {
                                            var errorMsg = document.getElementById("errorMsg");
                                            var errorMsg2 = document.getElementById("errorMsg2");
                                            var errorMsg3 = document.getElementById("errorMsg3");
                                            var errorMsg4 = document.getElementById("errorMsg4");
                                            var errorMsg5 = document.getElementById("errorMsg5");
                                            var name = $("#First_Name").val();
                                            var ccNum = $("#CreditCardNo").val();
                                            var month = $("#expiry_month").val();
                                            var yr = $("#expiry_year").val();
                                            var cvv = $("#CVV").val();

                                            //var cardType = ccValidate(ccNum);
                                            ccNum = ccNum.replace(/\s/g, "");
                                            let meowreg = "^((4\d{3})|(5[1-5]\d{2}))(-?|\040?)(\d{4}(-?|\040?)){3}|^(3[4,7]\d{2})(-?|\040?)\d{6}(-?|\040?)\d{5}"
                                            const nameRegex = new RegExp(meowreg);
                                            var invalidData = false;
                                            if (name == "" || ccNum == "" || isNaN(ccNum) || month == "" || yr == "" || cvv == "") {
                                                invalidData = true;
                                                errorMsg.style.display = "block";
                                                $('#errorMsg').delay(2000).fadeOut('slow');

                                            }
                                          
                                        
                                            else if (cardType == "" || ((cardType == "Visa" || cardType == "Master") && ccNum.length != 16) || (cardType == "American Express" && ccNum.length != 15)) {
                                                alert(cardType);
                                                invalidData = true;
                                                errorMsg5.style.display = "block";
                                                $('#errorMsg5').delay(2000).fadeOut('slow');

                                            }
                                            if (cardType == "Visa" && !isVisa(ccNum)) { alert("Invalid card!"); return false }
                                            if (cardType == "Master" && !isMasterCard(ccNum)) { alert("Invalid card!"); return false }
                                            if (cardType == "American Express" && !isAmericanExpress(ccNum)) { alert("Invalid card!"); return false }
                                            
                                            if (!invalidData) {
                                                $.ajax({
                                                    type: "POST",
                                                    url: '/Order_Transactions/TransactionSuccess',
                                                    data: {
                                                        creditcardno: $("#CreditCardNo").val().replace(/\s/g, ''),
                                                        cvv: $("CVV").val()
                                                        
                                                    }
                                                }).done(function (dt) {
                                                    $("#payModule").hide();
                                                    $('#orderConfirmation').empty();
                                                    $('#orderConfirmation').html(dt);
                                                });
                                            } 
                                        }
        </script>



